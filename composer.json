  "version": "1.1.1",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Andreev",
      "email": "p.andreev@leko.team"
    },
    {
      "name": "Oleg Kolzhanov",
      "email": "o.kolzhanov@leko.team"
    },
